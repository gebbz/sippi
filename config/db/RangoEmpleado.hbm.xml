<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class dynamic-insert="false" dynamic-update="false" mutable="true" name="modelo.RangoEmpleado" optimistic-lock="version" polymorphism="implicit" select-before-update="false">
    <id column="ID_RANGO_EMPLEADO" name="id" type="integer">
      <generator class="increment"/>
    </id>
    <property column="NOMBRE" name="nombre" type="string"/>
    <property column="COSTO_X_HORA" name="costoXHora" type="double"/>
  </class>
</hibernate-mapping>
