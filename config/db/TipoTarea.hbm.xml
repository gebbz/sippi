<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class dynamic-insert="false" dynamic-update="false" mutable="true" name="modelo.TipoTarea" optimistic-lock="version" polymorphism="implicit" select-before-update="false">
    <id column="ID_TIPO_TAREA" name="id" type="integer">
      <generator class="increment"/>
    </id>
    <property column="NOMBRE" name="nombre" type="string"/>
    <property column="CANT_OPERARIOS_PREDETERMINADA" name="cantOperariosPredeterminada" type="integer"/>
    <property column="CANT_HORAS_PREDETERMINADA" name="cantHorasPredeterminada" type="integer"/>
    <many-to-one class="modelo.RangoEmpleado" column="ID_RANGO_EMPLEADO" name="rangoEmpleadoPredeterminado"/>
  </class>
</hibernate-mapping>
