<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="modelo.RecursoXProveedor">
    <id column="ID_RXP" name="id" type="integer">
      <generator class="increment"/>
    </id>
    <many-to-one column="ID_PROVEEDOR" name="proveedor"/>
    <list cascade="all" name="listaPrecios" table="RXP_X_PSC">
      <key column="ID_RXP"/>
      <index column="idx"/>
      <one-to-many class="modelo.PrecioSegunCantidad"/>
    </list>
  </class>
</hibernate-mapping>
