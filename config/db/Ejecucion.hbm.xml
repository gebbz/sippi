<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class dynamic-insert="false" dynamic-update="false" mutable="true" name="modelo.Ejecucion" optimistic-lock="version" polymorphism="implicit" select-before-update="false">
    <id column="ID_Ejecucion" name="id" type="integer">
      <generator class="increment"/>
    </id>
    <property name="fechaInicio" type="date"/>
    <property name="fechaFin" type="date"/>
    <property name="observaciones" type="string"/>
    
    <list cascade="all-delete-orphan" name="listaTareas" table="TAREAS_X_EJECUCION">
      <key column="ID"/>
      <index column="ID_TAREAS_X_EJECUCION"/>
      <one-to-many class="modelo.TareaEjecucion"/>
    </list>
  </class>
</hibernate-mapping>
