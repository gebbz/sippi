<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class dynamic-insert="false" dynamic-update="false" mutable="true" name="modelo.Etapa" optimistic-lock="version" polymorphism="implicit" select-before-update="false">
    <id column="IDETAPA" name="id" type="integer">
      <generator class="increment"/>
    </id>
    <property name="nombre" type="string"/>
    <property name="duracion" type="string"/>
    <property name="fechaInicio" type="date"/>
    <property name="fechaFin" type="date"/>
    <property name="descripcion" type="string"/>
    <property name="nivelDeCriticidad" type="int"/>
    <list cascade="all" name="cpm" table="ETAPA_X_PREDECESORA">
      <key column="ID_ETAPA"/>
      <index column="idx"/>
      <one-to-many class="modelo.Etapa"/>
    </list>
    <property name="ubicacion" type="string"/>
    <many-to-one column="IDTRANSPPASAJEROS" name="transportePasajeros" class="modelo.TransporteDePasajeros"/>
    <many-to-one column="IDTRANSPMYH" name="tranportMyH" class="modelo.TransporteDeMaterialesYHerramientas"/>
    <list cascade="all" name="tareas" table="ETAPA_X_TAREAS">
      <key column="ID_ETAPA"/>
      <index column="idx"/>
      <one-to-many class="modelo.Tarea"/>
    </list>
  </class>
</hibernate-mapping>
