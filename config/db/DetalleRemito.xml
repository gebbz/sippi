<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class dynamic-insert="false" dynamic-update="false" mutable="true" name="modelo.DetalleRemito" optimistic-lock="version" polymorphism="implicit" select-before-update="false">
    <id column="ID_DETALLEREMITO" name="id" type="integer" unsaved-value="0">
      <generator class="increment"/>
    </id>
    <property column="CANTIDAD" name="cantidad" type="long"/>
    <property column="DESCRIPCION" name="descripcion" type="string"/>
    <many-to-one class="modelo.DetalleOrdenDeCompra" column="DETALLEOC" name="detalleOC"/>
  </class>
</hibernate-mapping>
