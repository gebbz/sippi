<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class dynamic-insert="false" dynamic-update="false" mutable="true" name="modelo.Remito" optimistic-lock="version" polymorphism="implicit" select-before-update="false">
    <id column="ID_REMITO" name="id" type="integer" unsaved-value="0">
      <generator class="increment"/>
    </id>
    <property name="fechaEntrega" type="date" column="FECHAENTREGA"/>
    <many-to-one class="modelo.OrdenDeCompra" column="ORDEN" name="orden"/>
    <list cascade="all" name="detalle" table="REMITO_X_DETALLEREMITO">
      <key column="ID_REMITO"/>
      <index column="idx"/>
      <one-to-many class="modelo.DetalleRemito"/>
    </list>
  </class>
</hibernate-mapping>
