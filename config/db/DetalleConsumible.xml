<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class dynamic-insert="false" dynamic-update="false" mutable="true" name="modelo.DetalleConsumible" optimistic-lock="version" polymorphism="implicit" select-before-update="false">
    <id column="IDDETALLECONSUMIBLE" name="id" type="integer">
      <generator class="increment"/>
    </id>
    <property column="DESCRIPCION" name="descripcion" type="string"/>
    <property column="CANTIDAD" name="cantidad" type="integer"/>
    <many-to-one class="modelo.Consumible" column="IDCONSUMIBLE" name="consumible"/>
  </class>
</hibernate-mapping>
