<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class dynamic-insert="false" dynamic-update="false" mutable="true" name="modelo.Telefono" optimistic-lock="version" polymorphism="implicit" select-before-update="false">

    <id column="ID_TEL" name="id" type="integer">
      <generator class="increment"/>
    </id>

    <property column="NUMERO" name="numero" type="string"/>

    <many-to-one name="tipo" class="modelo.TipoTelefono" column="TIPO_TEL_ID"/>

  </class>
</hibernate-mapping>
