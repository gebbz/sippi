<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class dynamic-insert="false" dynamic-update="false" mutable="true" name="modelo.EjecucionXHerramientaXDia" optimistic-lock="version" polymorphism="implicit" select-before-update="false" lazy="false">
    
        <id column="ID_EJECUCION_HERAMIENTA_DIA" name="id" type="integer">
            <generator class="increment"/>
        </id>

        <property column="FECHA" name="fecha" type="date"/>
        <property column="HORAS_UTILIZADAS" name="horasUtilizadas" type="integer"/>
    
    </class>
</hibernate-mapping>
