<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="modelo.PrecioSegunCantidad"  lazy="false">
    <id column="ID_PSC" name="id" type="integer">
      <generator class="increment"/>
    </id>
    <property name="fecha" not-null="true" type="date"/>
    <property name="cantidad" not-null="true" type="double"/>
    <property name="precio" not-null="true" type="double"/>
    <property name="fechaVigencia" type="date"/>
  </class>
</hibernate-mapping>
