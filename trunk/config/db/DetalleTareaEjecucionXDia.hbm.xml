<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class dynamic-insert="false" dynamic-update="false" mutable="true" name="modelo.DetalleTareaEjecucionXDia" optimistic-lock="version" polymorphism="implicit" select-before-update="false" lazy="false">
    
        <id column="ID_DETALLETAREAEJECXDIA" name="id" type="integer">
            <generator class="increment"/>
        </id>

        <property name="fecha" type="date"/>
        <property name="cantHorasNormales" type="double"/>
        <property name="cantHorasAl50" type="double"/>
        <property name="cantHorasAl100" type="double"/>
    
    </class>
</hibernate-mapping>
