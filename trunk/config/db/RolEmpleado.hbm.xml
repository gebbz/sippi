<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class dynamic-insert="false" dynamic-update="false" mutable="true" name="modelo.RolEmpleado" optimistic-lock="version" polymorphism="implicit" select-before-update="false">
    <id column="IDETAPA" name="id" type="integer">
      <generator class="increment"/>
    </id>
    <many-to-one column="IDEMPLEADO" name="empleado"/>
    <many-to-one column="IDESPECIALIDAD" name="especialidad"/>
    <property name="esLider" type="boolean"/>
  </class>
</hibernate-mapping>
