<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class dynamic-insert="false" dynamic-update="false" mutable="true" name="modelo.TipoTarea" optimistic-lock="version" polymorphism="implicit" select-before-update="false">
    <id column="ID_TIPO_TAREA" name="id" type="integer">
      <generator class="increment"/>
    </id>
    <property column="NOMBRE" name="nombre" type="string"/>
    
<!--    <many-to-one class="modelo.RangoEmpleado" column="ID_RANGO_EMPLEADO" name="rangoEmpleadoPredeterminado"/>-->
    <list cascade="all" name="detalleSOXTareaPredeterminados" table="TIPO_TAREA_X_DETALLESOXTAREAPREDETER">
      <key column="ID_TIPO_TAREA"/>
      <index column="idx"/>
      <one-to-many class="modelo.SubObraXTarea"/>
    </list>
  </class>
</hibernate-mapping>
