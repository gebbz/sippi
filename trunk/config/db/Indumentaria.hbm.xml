<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class dynamic-insert="false" dynamic-update="false" mutable="true" name="modelo.Indumentaria" optimistic-lock="version" polymorphism="implicit" select-before-update="false">
    <id column="IDINDUMENTARIA" name="id" type="integer">
      <generator class="increment"/>
    </id>
    <property column="DESCRIPCION" name="descripcion" type="string"/>
    <many-to-one column="ID_TALLE" name="talle"/>
    <many-to-one column="ID_TIPO_INDUMENTARIA" name="tipo"/>
  </class>
</hibernate-mapping>
