<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class dynamic-insert="false" dynamic-update="false" mutable="true" name="modelo.SubObraXTarea" optimistic-lock="version" polymorphism="implicit" select-before-update="false">
    <id column="ID_SUBOBRA_X_TAREA" name="id" type="integer">
      <generator class="increment"/>
    </id>
    <property column="OBSERVACIONES" name="observaciones" type="string"/>
    <property column="NOMBRE" name="nombre" type="string"/>
    <many-to-one class="modelo.TipoTarea" column="ID_TIPO_TAREA" name="tipoTarea"/>
  </class>
</hibernate-mapping>
