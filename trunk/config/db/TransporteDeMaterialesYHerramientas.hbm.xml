<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class dynamic-insert="false" dynamic-update="false" mutable="true" name="modelo.TransporteDeMaterialesYHerramientas" optimistic-lock="version" polymorphism="implicit" select-before-update="false" table="TRANSPORTE_MYH">
    <id column="IDTransporteDeMyH" name="id" type="integer">
      <generator class="increment"/>
    </id>
    <property column="PRECIO" name="precio" type="float"/>
    <many-to-one class="modelo.Domicilio" column="ID_DOMICI_ORI" name="origen"/>
    <many-to-one class="modelo.Domicilio" column="ID_DOMICI_DES" name="destino"/>
    <many-to-one class="modelo.Proveedor" column="IDProveedor" name="empresa"/>
    <list cascade="all" name="herramientas" table="TMYH_X_HDE">
      <key column="ID_TMYH_X_HDE"/>
      <index column="idx"/>
      <many-to-many class="modelo.HerramientaDeEmpresa" column="ID_HdE"/>
    </list>
    <list cascade="all" name="detallesMaterial" table="TMYH_X_DM">
      <key column="ID_TMYH_X_DM"/>
      <index column="idx"/>
      <many-to-many class="modelo.DetalleMaterial" column="ID_DM"/>
    </list>
  </class>
</hibernate-mapping>
