<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class dynamic-insert="false" dynamic-update="false" mutable="true" name="modelo.TipoDocumento" optimistic-lock="version" polymorphism="implicit" select-before-update="false">
      <id name="id" type="integer" column="ID">
          <generator class="increment"></generator>
      </id>
      <property name="nombre" type="string" column="NOMBRE"/>
      <property name="descripcion" type="string" column="DESCRIPCION"/>
  </class>
</hibernate-mapping>
