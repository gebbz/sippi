	<subDataset name="DatosCotizacion" whenResourceMissingType="Empty">
		<queryString language="hql">
			<![CDATA[SELECT
	Cot.nroCotizacion AS nroCotizacion,
	Cot.descripcion AS descripcion,
	Cot.lugarEntrega AS lugarEntrega,
	Cot.plazoEntrega AS plazoEntrega,
	Cot.validezOferta AS validezOferta
FROM
	Cotizacion as Cot
WHERE
	Cot.id=1]]>
		</queryString>
	</subDataset>
	<subDataset name="SubObra">
		<queryString language="hql">
			<![CDATA[SELECT
	SO.nombre AS nombreSO,
	SO.descripcion AS descripcionSO
FROM
	SubObra as SO
WHERE
	SO.id IN (	 SELECT SO.id FROM	Cotizacion as Cot LEFT JOIN Cot.subObras as SO WHERE Cot.id=1)]]>
		</queryString>
	</subDataset>
